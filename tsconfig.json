{
    "compilerOptions": {
        "outDir": "./build",
        "noImplicitAny": true,
        "module": "ES6",
        "target": "ES5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "lib": [ "DOM", "ES2015", "ScriptHost", "ES2019.Array", "es2020", "DOM.Iterable" ],
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx"
    ]
}